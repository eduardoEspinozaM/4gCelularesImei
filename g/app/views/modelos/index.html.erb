
<div class="col-md-9 ms-paper-content-container">
    <div class="ms-paper-content">
        <h1>Marcas</h1>
        <p id="notice"><%= notice %></p>
        <section class="ms-component-section">
            <div class="bs-example">
                <table class="table">
                    <thead>
                        <tr>
                        <th>Marca</th>
                        <th>Nombre</th>
                            <th colspan="3"></th>
                        </tr>
                    </thead>
                    <tbody>
                        
    <% @modelos.each do |modelo| %>
                        <tr>
                            <td><%= modelo.marca_id %></td>
        <td><%= modelo.nombre %></td>
        <td><%= link_to 'Show', modelo %></td>
        <td><%= link_to 'Edit', edit_modelo_path(modelo) %></td>
        <td><%= link_to 'Destroy', modelo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
                        <% end %>
                    </tbody>
                </table>
            </div>
            <!-- panel-code -->
        </section>
    </div>
    <!-- ms-paper-content -->
</div>


