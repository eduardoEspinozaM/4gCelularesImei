<p id="notice"><%= notice %></p>

<h1>Forma Pagos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @forma_pagos.each do |forma_pago| %>
      <tr>
        <td><%= forma_pago.nombre %></td>
        <td><%= link_to 'Show', forma_pago %></td>
        <td><%= link_to 'Edit', edit_forma_pago_path(forma_pago) %></td>
        <td><%= link_to 'Destroy', forma_pago, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Forma Pago', new_forma_pago_path %>
