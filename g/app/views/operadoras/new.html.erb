<div class="col-md-9 ms-paper-content-container">
    <div class="ms-paper-content">
        <h1>Operadora <small class="text-muted">Crear nuevo</small></h1>
        <% if notice %>
            <div class="alert alert-success alert-dismissible" role="alert">
                <p><%= notice %></p>
            </div>
        <%end%>

        <section class="ms-component-section">
            <div class="bs-example">
            	<%= render 'form', operadora: @operadora %>  
            	<%= link_to 'Volver', operadoras_path, class: 'btn btn-default btn-raised' %>
            </div>
            <!-- panel-code -->
        </section>
    </div>
    <!-- ms-paper-content -->
</div>
