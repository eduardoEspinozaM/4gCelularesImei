     <div class="col-lg-8 col-lg-offset-2 well">
<fieldset>
                    <table class="table table-striped table-striped-primary">
               <p id="notice"><%= notice %></p>
                    <h1>Marcas</h1>

                      <tbody>
                        <tr>
                           <td>Operadora</td>
                           <td>Precio</td>
                           <td colspan="3"></td>
                           <td><%= link_to 'New Operadora', new_operadora_path %></td>
                       
                        </tr>

                        <% @operadoras.each do |operadora| %>

                        <tr>   
        <td><%= operadora.nombre %></td>
        <td><%= operadora.costo %></td>
        <td><%= link_to 'Show', operadora %></td>
        <td><%= link_to 'Edit', edit_operadora_path(operadora) %></td>
        <td><%= link_to 'Destroy', operadora, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr> </tr>
                        <% end %>
                      
                      </tbody>
                    </table>
                  </div>
                  </fieldset>             
