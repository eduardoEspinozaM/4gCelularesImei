


    




     <div class="col-lg-8 col-lg-offset-2 well">
<fieldset>


                  <table id="link_to" class="table table-striped table-bordered ">
                     <div class=" table-responsive">
                        <thead>
                           <tr>
                              <td>Modelo</td>
                           <td>Tipo</td>
                           <td colspan="3"></td>
                           <td><%= link_to 'Nuevo Modelo', new_telefono_path %></td>
                       
                           </tr>
                           </h2>
                        </thead>
                        <tbody>
                             <% @telefonos.each do |telefono| %>
      <tr>
        <td><%= telefono.modelo.nombre %></td>
        <td><%= telefono.tipo.nombre %></td>
        <td><%= link_to 'Show', telefono %></td>
        <td><%= link_to 'Edit', edit_telefono_path(telefono) %></td>
        <td><%= link_to 'Destroy', telefono, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr> <% end %>
                      
                        </tbody>
                  </table>
                  </div>
              
<script>
   $(document).ready(function() {
     $('#link_to').DataTable( {
         responsive: true,
         "bProcessing": true,
         //"sPaginationType": "full_numbers",
